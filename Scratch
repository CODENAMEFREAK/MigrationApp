printf("Hello 1\n");
        int rc = virDomainGetJobStats(domainToMigrate,&info.type,&params,&nparams,flags);
	unsigned long long value =0;
	printf("Hello2 \n");		
        while(1)
        {
	printf("Hello 3\n");
            if(rc==0)
            {
                printf("Got information");
		virTypedParamsGetULLong(params,nparams,VIR_DOMAIN_JOB_MEMORY_DIRTY_RATE,&value);
        	printf("No. of pages dirtied -> %llu\n",value);
                //printInformation(params,nparams);
		sleep(2);
                
            }
            else{
		sleep(2);
                printf("Could not get rc\n");
		
            }
	rc = virDomainGetJobStats(domainToMigrate,&info.type,&params,&nparams,flags);
            
        }
